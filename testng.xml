<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" verbose="2" allow-return-values="true"
	preserve-order="true">
	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
		<listener class-name="com.selenium.test.listener.TestNGListener" />
	</listeners>
	<test name="Stock" preserve-order="true">
		<classes>
			<class name="com.selenium.test.stock.CategoryManagement" />
			<class name="com.selenium.test.stock.AdminTestLiveMats" />
			<class name="com.selenium.test.stock.AdminPermanentMats" />
			<class name="com.selenium.test.stock.TestDayAllocation" />								
		</classes>
	</test>

</suite> 