<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" verbose="2" allow-return-values="true"
	preserve-order="true">

	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
		<listener class-name="com.selenium.test.listener.TestNGListener" />
	</listeners>

	<test name="TestCenter" preserve-order="true">
		<classes>
      		<class name="com.selenium.test.testcenter.TestSessionList">
      			   <methods>
         				<include name = "step04_CreateLastSession" />
         				<include name = "step06_LogOut" />
         				<include name = "step05_SearchLastSession" />
      			  </methods>
      		</class>
		</classes>
	</test>

	<test name="Examiner" preserve-order="true">
		<classes>
			<class name="com.selenium.test.examiner.AvailabilitySummary" />
			<class name="com.selenium.test.examiner.ExaminerTask" />
		</classes>
	</test>
	
	<test name="PreTestPlanning" preserve-order="true">
		<classes>
			<class name="com.selenium.test.pretestplanning.ResourcePlanning"/>
		</classes>
	</test>
	
	<test name="Examiner" preserve-order="true">
		<classes>
			<class name="com.selenium.test.examiner.SpkDeployment" />
		</classes>
	</test>
			
	<test name="TestCenter" preserve-order="true">
		<classes>
      		<class name="com.selenium.test.testcenter.SpkRoomTemplate"/>
      		<class name="com.selenium.test.testcenter.SpkRoomAvailability"/>
		</classes>
	</test>
	
	<test name="PreTestArrange" preserve-order="true">
		<classes>
      		<class name="com.selenium.test.pretestarrange.SpkArrangement"/>
		</classes>
	</test>

</suite> 

 